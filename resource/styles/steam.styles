				2="fill( x0  , y0+1, x0+1, y1  , InBorderShade )"  //l
				3="fill( x0+1, y1-1, x1  , y1  , InBorderLight )"  //b
				4="fill( x1-1, y0+1, x1  , y1-1, InBorderLight )"  //r
			}
		}
		
		"Page CheckButtonList:scrollbar"
		{
			inset="1 1 1 1"
			textcolor="text2"
			bgcolor=none
			render
			{
				//gradient to obscure content at top of scrolling region 
				//0="gradient( x0+1, y0, x1+1, y0 + 6, FrameShade, none  )"
				
				//gradient to obscure content at bottom of scrolling region 
				1="gradient( x0, y1 - 9, x1+1, y1-1, none, FrameShade )"
			}
			
			render_bg
			{
				0="fill( x0, y0, x1, y1, dark )"
				
				1="fill( x0  , y0  , x1-1, y0+1, InBorderShade )"  //t
				2="fill( x0  , y1-1, x1  , y1  , InBorderLight )"  //b
				3="fill( x0  , y0+1, x0+1, y1-1, InBorderShade )"  //l
				4="fill( x1-1, y0  , x1  , y1-1, InBorderLight )"  //r
