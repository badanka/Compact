		CheckButton
		{
			font-family=basefont
			font-size=13
			font-weight=400
			textcolor="Label"
			image="graphics/chkunsel_1"
		}
		
		CheckButton:hover
		{
			image="graphics/chkunsel_2"    
		}
		
		CheckButton:focus
		{
			textcolor="Text"
			bgcolor=none
			image="graphics/chkunsel_2"  
		}
		
		CheckButton:selected
		{
			image="graphics/chksel_1"    
		}
		
		CheckButton:selected:hover
		{
			image="graphics/chksel_2"    
		}
		
		CheckButton:selected:active
		{
			image="graphics/chksel_3"    
		}
		
		CheckButton:disabled
		{
			textcolor="TextDisabled"
			bgcolor=none
			image="graphics/chkunsel_0"
		}
		
		CheckButton:selected:focus
		{
			textcolor="Text"
			render_bg {}
			bgcolor=none
			image="graphics/chksel_2"
		}
		
		CheckButton:selected:disabled
		{
			textcolor="TextDisabled"
			bgcolor=none
			image="graphics/chksel_0"
		}
		
		IndeterminateCheckButton
		{
			font-family=basefont
			font-size=13
			font-weight=400
			textcolor="Label"
			bgcolor=none
			image="graphics/chkIndeterminate"
		}
		
		CheckButtonList
		{
			inset="1 1 1 1"
			textcolor="text2"
			bgcolor=none
			render_bg
			{
				0="fill( x0, y0, x1, y1, FrameBG1 )"
				
				1="fill( x0  , y0  , x1-1, y0+1, InBorderShade )"  //t
				2="fill( x0  , y1-1, x1  , y1  , InBorderLight )"  //b
				3="fill( x0  , y0+1, x0+1, y1-1, InBorderShade )"  //l
				4="fill( x1-1, y0  , x1  , y1-1, InBorderLight )"  //r
			}
		}
		
		CheckButtonList:scrollbar
		{
			inset="1 1 1 1"
			textcolor="text2"
			bgcolor=none
			render
			{
				//gradient to obscure content at top of scrolling region 
				0="gradient( x0+1, y0, x1+1, y0 + 6, almostblack, none  )"
				
				//gradient to obscure content at bottom of scrolling region 
				1="gradient( x0, y1 - 9, x1+1, y1-1, none, almostblack )"
			}
			render_bg
			{
				// background fill
				0="gradient( x0 + 1, y0 + 1, x1 - 1, y0 +80, dialogbg, BackgroundStartOpaque )"
				1="fill( x0 + 1, y0 + 80, x1 - 1, y1 - 1, BackgroundStartOpaque )"
				2="gradient( x0 + 1, y0 + 1, x1 - 1, y0 + 30, backgroundstartsubtle, none )"
				
				// lines around
				3="fill( x0 + 2, y0, x1 - 2, y0 + 1, ButtonBorderDisabled )"  //t
				4="fill( x0 + 2, y1 - 1, x1 - 2, y1, ButtonBorderDisabled )"  //b
				5="fill( x0, y0 + 2, x0 + 1, y1 - 2, ButtonBorderDisabled )"  //l
				6="fill( x1 - 1, y0 + 2, x1, y1 - 2, ButtonBorderDisabled )"  //r
				
				
				// single pixel fills in the corners
				7="fill( x0 + 1, y0 + 1, x0 + 2, y0 + 2, ButtonBorderDisabled )"
				8="fill( x1 - 2, y0 + 1, x1 - 1, y0 + 2, ButtonBorderDisabled )"
				9="fill( x0 + 1, y1 - 2, x0 + 2, y1 - 1, ButtonBorderDisabled )"
				10="fill(x1 - 2, y1 - 2, x1 - 1, y1 - 1, ButtonBorderDisabled )"
				11="fill(x0, y0 + 1, x0 + 1, y0 + 2, ButtonBorderDisabled2 )"
				12="fill(x1 - 1, y0 + 1, x1, y0 + 2, ButtonBorderDisabled2 )"
				13="fill(x0, y1 - 2, x0 + 1, y1 - 1, ButtonBorderDisabled2 )"
				14="fill(x1 - 1, y1 - 2, x1, y1 - 1, ButtonBorderDisabled2 )"
				15="fill(x0 + 1, y0, x0 + 2, y0 + 1, ButtonBorderDisabled2 )"
				16="fill(x1 - 2, y0, x1 - 1, y0 + 1, ButtonBorderDisabled2 )"
				17="fill(x0 + 1, y1 - 1, x0 + 2, y1, ButtonBorderDisabled2 )"
				18="fill(x1 - 2, y1 - 1, x1 - 1, y1, ButtonBorderDisabled2 )"
			}
		} 
		
		
