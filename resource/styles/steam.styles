		//main frame
		Frame
		{
			bgcolor="MainFrameBG4"
			render
			{
				0="fill(x0  , y0  , x1  , y0+1, MainFrameBG4 )"	//top
				1="fill(x0  , y0  , x0+1, y1  , MainFrameBG4 )"	//left
				2="fill(x1-1, y0  , x1  , y1  , MainFrameBG4 )"	//right 
				3="fill(x0  , y1-1, x1+1, y1  , MainFrameBG4 )"	//bottom
			}
		} 
		
		Frame:FrameFocus
		{
			bgcolor="MainFrameBG2"
			
			render
			{
				0="gradient_horizontal( x0   , y0   , x0+75, y0+1 , ExBorderHighlight,	ExBorderLight )"	//top
				1="gradient_horizontal( x0+75, y0   , x1+1 , y0+1 , ExBorderLight,		ExBorderShade )"	//top

				2="gradient(			x0   , y0   , x0+1 , y0+50, ExBorderHighlight,	ExBorderLight )"	//left
				3="gradient(			x0   , y0+50, x0+1 , y1   , ExBorderLight,		ExBorderShade )"	//left

				4="gradient(			x1-1 , y0   , x1   , y1   , ExBorderShade,		ExBorderDark )"		//right 
				5="gradient_horizontal( x0   , y1-1 , x1+1 , y1   , ExBorderShade,		ExBorderDark )"		//bottom
			}
		
			render_bg
			{
				0="fill(				x0+1, y0    , x1-1 , y0+28, MainFrameBG1 )"				// 1 pixel border, 20 pixels titlebar, 2 pixel border below, 
																								// then 6 pixels of MFBG1
				1="gradient(			x0+1, y0+28 , x1-1 , y0+48, MainFrameBG1,MainFrameBG2 )"	// 
				2="fill(				x0+1, y0+48 , x1-1 , y1-1 , MainFrameBG2 )"				// 
